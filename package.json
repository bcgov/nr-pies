    "docusaurus-json-schema-plugin": "1.14.0",
    "docusaurus-lunr-search": "3.6.0",
    "mermaid": "11.6.0",
    "prism-react-renderer": "2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
